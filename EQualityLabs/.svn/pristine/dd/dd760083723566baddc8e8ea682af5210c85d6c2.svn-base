@import url("https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css");
@import url("font-awesome.min.css");






 
/* Reset CSS Begin */
*{box-sizing: border-box;}
ul, li, ol{padding:0; margin:0;}
li{list-style:none;}
/* Reset CSS End */

 
/* Comman Class Begin */
/* Comman Class End */

 
/* Basic Structure Begin */
/* Basic Structure End */

 
/* Header */
header{padding:10px 0;}
header .mainnav-dropdown{margin:10px 0 0;}
header .navbar-light .navbar-toggler{padding:.60rem .60rem; border-radius:0;}
header .navbar-light .navbar-toggler{background-color:#06f; border-color:#06f;}
header .navbar-light .navbar-toggler[aria-expanded="false"]{background-color:#fff; border-color:#ddd;}
.logo{width:58px; height:58px; float:left;}
.logo img{width:100%; float:left;}
/* Main Navigation */
.navbar-light{ padding: 3%; padding-left: 13%; margin-left: 0%; background-color:none; border-bottom: 1px solid #e7e7e7; font-weight: 500;}
.collaspe > ul > li > a, .navbar-light .navbar-nav .nav-link{color: #005476;}
.collaspe > ul > li > a, .navbar-light .navbar-nav .active>.nav-link{color: #005476;}
.mainnav-dropdown, .mainnav-dropdown.navbar-light .navbar-toggler, #navbarNav{float:right;}
.mainnav-dropdown{display:block; padding:0; border:0;}
.main-nav{clear:both; border-top:10px solid #06f; width:100%;}
.navbar-nav li a{text-indent:15px;}
.navbar-nav li:hover{background-color:#005476;}
.navbar-nav li:hover a{color:#fff;}
.mainnav-dropdown + .main-nav{margin-top:-2px; position:relative; z-index:2;}
/* Jumbotron */
.jumbotron-holder{overflow:hidden; padding:2vh 0 0; text-align:center; position:absolute; left:0; bottom:15px; width:100%;}
#bgImg{position:relative; overflow:hidden;}
#bgImg img{width:100%; float:left; min-height:200px; margin:0 0;}
#signUpButton{font-size:14px;}
.jumbotron{background-color: white;}
.jumbotron > .display-10 , .lead{text-align: center;}
/* Content Panel */
#content-panel{padding-top:20px; padding-bottom:50px; overflow:hidden;}
/* Footer */
footer{clear: both;}
.footer-up{background:#00385E; padding-top:25px; padding-bottom:25px;}
.footer-down{background:black; font-size:12px; text-align:center; padding:13px 0;}
.social-connect h2{font-size:1rem;color: #ffffff;}
.social-links, .footer-navigation ul{float:left;}
.social-links li, .social-links li a{float:left;}
.social-links li{list-style:none; margin:0 0 0 10px;}
.social-links li:first-child{margin:0;}
.social-links li a{}
.social-links li a i{font-size:16px; background:#eee; width:26px; height:26px; color:#666; border-radius:50%; text-align:center; line-height:26px;}
.social-links li a:hover .fa{color:#fff;}
.social-links li a:hover .fa-facebook{background:#3b5998;}
.social-links li a:hover .fa-twitter{background:#55acee;}
.social-links li a:hover .fa-linkedin{background:#007bb5;}
.social-links li a:hover .fa-google-plus{background:#dd4b39;}
.social-links li a:hover .fa-pinterest-p{background:#cb2027;}
.social-links li a:hover .fa-instagram{background:#e4405f;}
.footer-navigation ul{width:27%; float:left; min-height:110px;}
.footer-navigation ul li{overflow:hidden;clear: both; padding:3px 0;}
.footer-navigation ul li a{color:white; float:left; font-size:14px; line-height:14px; border-left:3px solid #ddd; padding:0 0 0 10px;}
.footer-navigation ul li a:hover{color:#555; text-decoration:none;}
.footer-down .footname, .footer-down .foot-links{line-height:12px;}
.footer-down-hold, .footer-down .foot-links, .footer-down a{font-size:12px; color:#ffffff;}
.foot-links{display:inline-block;}
.footer-down a{border-left:1px solid #ccc; padding:0 10px; line-height:8px;}
.footer-down a:first-child{margin-left: 10px;}
.footer-down a:hover{color:#fff;}

/* Pricing Begin */
.columns {margin-left: 9%; float: left; width: 20.3%; padding: 8px;}
.price {list-style-type: none;border: 1px solid #eee; margin: 0; padding: 0; -webkit-transition: 0.3s; transition: 0.3s;}
.price:hover {box-shadow: 0 8px 12px 0 rgba(0,0,0,0.2)}
.price .header{background-color: #111;color: white;font-size: 25px;}
.price li {border-bottom: 1px solid #eee;padding: 20px;text-align: center;}
.price .grey {background-color: #eee;font-size: 20px;}
.button {background-color: #4CAF50;border: none;color: white;padding: 10px 25px;text-align: center;text-decoration: none;font-size: 18px;}





          @media only screen and (min-width :320px) and (max-width: 767px) {
            .columns{
                margin-left: 0%;
            }
          }


          @media only screen and (min-width: 768px){
            .logo{width:70px; height:70px;}
            .main-nav{display:block; margin-top:10px; clear:none; border-top:0;}
            .navbar-nav li a{text-indent:0;}
            .mainnav-dropdown{float:right;}
            .jumbotron-holder{padding:20vh 0;}
            #bgImg img{margin:-9% 0; min-height:none;}
            .navbar-nav li{border-radius:3px; overflow:hidden;}
            .navbar-light .navbar-nav .nav-link{padding-right:1rem; padding-left:1rem;}
            .navbar-light .navbar-nav .nav-link:hover{color:#ffffff;}
          }

           

            @media only screen and (max-width: 600px) {
                .columns {
                    width: 100%;
                }
            }
